Algoritmo Intercambiar
	
	Definir a,b Como Real;
	Definir aux Como Real;
	
	Escribir "Dime el valor de a ";
	Leer a;
	Escribir "Dime el valor de b ";
	Leer b;
	
	//Intercambiar sus valores
	aux <- a;
	a <- b;
	b <- aux;
		
	
	Escribir "El valor cambiado de a es ", a;
	Escribir "El valor cambiado de b es ", b;
	
	
FinAlgoritmo
